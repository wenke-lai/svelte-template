{
	"$schema": "https://railway.com/railway.schema.json",
	"build": {
		"builder": "RAILPACK"
	},
	"deploy": {
		"runtime": "V2",
		"numReplicas": 1,
		"limitOverride": {
			"containers": {
				"cpu": 2,
				"memoryBytes": 2000000000
			}
		},
		"sleepApplication": false,
		"multiRegionConfig": {
			"us-west2": {
				"numReplicas": 1
			}
		},
		"restartPolicyType": "ON_FAILURE",
		"restartPolicyMaxRetries": 10
	}
}
